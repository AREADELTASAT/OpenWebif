#filter WebSafe
<?xml version="1.0" encoding="UTF-8"?>
<e2movielist>
	#for $movie in $movies
		<e2movie>
			<e2servicereference>$movie.serviceref</e2servicereference>
			<e2title>$movie.filename</e2title>
			<e2description>$movie.description</e2description>
			<e2descriptionextended>-</e2descriptionextended>
			<e2servicename>-</e2servicename>
			<e2time>-</e2time>
			<e2length>-</e2length>
			<e2tags>-</e2tags>
			<e2filename>-</e2filename>
			<e2filesize>-</e2filesize>
		</e2movie>
	#end for
</e2movielist>
#end filter